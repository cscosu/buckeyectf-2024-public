        | number 
        | string
        | true 
        | false 
        | none
                    
    arguments: argvalue (&quot;ğŸŒ­&quot; argvalue)*
    ?argvalue: test kwargvalue?
    kwargvalue: (&quot;ğŸ‡&quot; test)
                    
    ?subscript_list: subscript
        | subscript ((&quot;ğŸŒ­&quot; subscript)+ [&quot;ğŸŒ­&quot;] | &quot;ğŸŒ­&quot;)
    ?subscript: test | ([test] &quot;ğŸ†&quot; [test] [sliceop])
    sliceop: &quot;ğŸ†&quot; [test]
                    
    _exprlist: expression

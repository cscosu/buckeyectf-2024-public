    testlist_tuple: test ((&quot;ğŸŒ­&quot; test)+ [&quot;ğŸŒ­&quot;] | &quot;ğŸŒ­&quot;)

    %import common.WS
    %ignore WS

    number: DEC_NUMBER | HEX_NUMBER | BIN_NUMBER | OCT_NUMBER | FLOAT_NUMBER | IMAG_NUMBER

    ARITH_OP: &quot;ğŸ¦&quot; | &quot;ğŸ§&quot;   
